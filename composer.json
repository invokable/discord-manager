{
  "name": "revolution/discord-manager",
  "description": "Discord Manager",
  "keywords": [
    "discord",
    "laravel"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/laravel-discord/Yasmin"
    },
    {
      "type": "vcs",
      "url": "https://github.com/laravel-discord/Collection"
    },
    {
      "type": "vcs",
      "url": "https://github.com/laravel-discord/EventEmitter"
    },
    {
      "type": "vcs",
      "url": "https://github.com/laravel-discord/Validator"
    }
  ],
  "require": {
    "php": "^7.2||^8.0",
    "charlottedunois/yasmin": "^0.7",
    "laravel-notification-channels/discord": "^1.0",
    "restcord/restcord": "^0.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0||^9.0",
    "mockery/mockery": "^1.3",
    "orchestra/testbench": "^4.0||^5.0"
  },
  "conflict": {
    "cakephp/core": "*"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\DiscordManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\DiscordManager\\Providers\\DiscordManagerServiceProvider"
      ],
      "aliases": {
        "DiscordManager": "Revolution\\DiscordManager\\Facades\\DiscordManager"
      }
    }
  }
}
