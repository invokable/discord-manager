{
    "name": "revolution/discord-manager",
    "description": "Discord Manager",
    "keywords": [
        "discord",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "restcord/restcord": "dev-develop",
        "illuminate/support": "^9.0",
        "team-reflex/discord-php": "^7.0||^10.0",
        "discord/interactions": "^2.2",
        "simplito/elliptic-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^7.0",
        "laravel-notification-channels/discord": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\DiscordManager\\": "src/"
        }
    },
    "suggest": {
        "laravel-notification-channels/discord": "Laravel Notification."
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\DiscordManager\\Providers\\DiscordManagerServiceProvider",
                "Revolution\\DiscordManager\\Providers\\DiscordInteractionsServiceProvider"
            ],
            "aliases": {
                "DiscordManager": "Revolution\\DiscordManager\\Facades\\DiscordManager"
            }
        }
    }
}
