{
  "name": "revolution/discord-manager",
  "description": "Discord Manager",
  "keywords": [
    "discord",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2||^8.0",
    "laravel-discord/yasmin": "^0.9",
    "laravel-notification-channels/discord": "^1.0",
    "restcord/restcord": "^0.4.0",
    "illuminate/support": "^6.0||^7.0||^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0||^9.0",
    "mockery/mockery": "^1.3",
    "orchestra/testbench": "^4.0||^5.0||^6.0"
  },
  "conflict": {
    "cakephp/core": "*"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\DiscordManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\DiscordManager\\Providers\\DiscordManagerServiceProvider"
      ],
      "aliases": {
        "DiscordManager": "Revolution\\DiscordManager\\Facades\\DiscordManager"
      }
    }
  }
}
