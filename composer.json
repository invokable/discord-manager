{
    "name": "revolution/discord-manager",
    "description": "Discord Manager",
    "keywords": [
        "discord",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/http": "^10.0||^11.0",
        "illuminate/support": "^10.0||^11.0",
        "discord/interactions": "^2.2",
        "simplito/elliptic-php": "^1.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^8.0||^9.0"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\DiscordManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\DiscordManager\\Providers\\DiscordInteractionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
