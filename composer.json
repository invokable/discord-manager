{
    "name": "revolution/discord-manager",
    "description": "Discord Manager",
    "keywords": [
        "discord",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/http": "^11.0||^12.0",
        "illuminate/support": "^11.0||^12.0",
        "discord/interactions": "^2.2",
        "simplito/elliptic-php": "^1.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0||^10.0",
        "laravel/pint": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\DiscordManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\DiscordManager\\Providers\\DiscordInteractionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
